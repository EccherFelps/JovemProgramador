@model EnderecoModel


<form>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Logradouro </label>
        <input type="text" class="form-control col-md-6" asp-for="logradouro" />
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Complemento</label>
        <input type="text" class="form-control col-md-4" asp-for="complemento" />
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Bairro</label>
        <input type="text" class="form-control col-md-2" asp-for="bairro" />
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Localidade</label>
        <input type="email" class="form-control col-md-2" asp-for="localidade" />

    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Uf</label>
        <input type="text" class="form-control col-md-2" asp-for="uf" />
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">DDD </label>
        <input type="text" class="form-control col-md-2" asp-for="ddd" />
    </div>
</form>

<div class="text-center">
    <a class="btn btn-danger col-1" role="button" asp-controller="Aluno" asp-action="Index">Voltar</a>
</div>


